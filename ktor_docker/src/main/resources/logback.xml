<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{YYYY-MM-dd HH:mm:ss.SSS} [%thread] %-5level [%X{X-CallId}] [%X{ip}] %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <root level="DEBUG">
        <appender-ref ref="STDOUT"/>
    </root>

    <logger name="io.netty" level="DEBUG"/>
    <logger name="org.eclipse.jetty" level="INFO"/>
    <logger name="org.eclipse.jetty.http.HttpGenerator" level="DEBUG"/>
</configuration>